{
  "name": "oauth2-js",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "test": "DEBUG='oauth2:*' node ./test/bin/www"
  },
  "main": "index.js",
  "dependencies": {
    "debug": "^1.0.4",
    "rsa-pem-from-mod-exp": "^0.8.4",
    "uid2": "0.0.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "body-parser": "^1.14.1",
    "cookie-parser": "^1.4.0",
    "eslint": "^0.24.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "jade": "^1.11.0",
    "morgan": "^1.6.1"
  },
  "repository": {
    "type": "git",
